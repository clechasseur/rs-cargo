{
  "problemMatcher": [
    {
      "owner": "rs-cargo",
      "pattern": [
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*([Ww]arning|[Ww]arn|[Ee]rror)(?:\\x1b\\[[\\d;]+m)*(\\[(.*)\\])?(?:\\x1b\\[[\\d;]+m)*:(?:\\x1b\\[[\\d;]+m)* ([^\\x1b]*)(?:\\x1b\\[[\\d;]+m)*$",
          "severity": 1,
          "message": 4,
          "code": 3
        },
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*\\s*(?:\\x1b\\[[\\d;]+m)*\\s*-->\\s+(?:\\x1b\\[[\\d;]+m)*([^:]+):(\\d+):(\\d+)(?:\\x1b\\[[\\d;]+m)*$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ]
    }
  ]
}
